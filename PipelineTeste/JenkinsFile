pipeline {
    agent any
    parameters {
        string(name: 'Branch_APP', defaultValue: 'FB_GEVO-', description: 'Branch da aplicação')
        choice(name: 'Aplicação', defaultValue: '', description: 'Informe a aplicação', choices: [SFI,SCI,SVI])
    }
    stages {
        stage('Build') {
            steps {
                echo 'Building..'
                echo "${params.Greeting} World!"
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}